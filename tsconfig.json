{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 12",
    "compilerOptions": {
        "lib": ["es2019", "ES2020.Promise", "ES2020.BigInt", "ES2020.String"],
        "target": "ES2019",
        "module": "CommonJS",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}